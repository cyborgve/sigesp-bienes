{
  "name": "sigesp_bienes",
  "version": "0.60.453-RC",
  "contributors": [
    {
      "name": "Richard Iribarren",
      "email": "richardiribarren@gmail.com",
      "url": "https://github.com/cyborgve"
    }
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "start:public": "ng serve --port 4300 --host 0.0.0.0 --disable-host-check",
    "docker:postgres": "docker-compose -f ./docker-sigesp-database/docker-compose.yml up -d",
    "docker:postgres:down": "docker-compose -f ./docker-sigesp-database/docker-compose.yml down",
    "docker:php": "docker-compose -f ./docker-sigesp-php/docker-compose.yml up -d",
    "docker:php:down": "docker-compose -f ./docker-sigesp-php/docker-compose.yml down",
    "build": "ng build --aot --build-optimizer --output-hashing=none --configuration production --base-href /SigespERP/bienes/",
    "build:stats": "ng build --aot --build-optimizer --output-hashing=none --configuration production --base-href /SigespERP/bienes/ --stats-json",
    "build:pruebas": "ng build --aot --build-optimizer --output-hashing=none --configuration production --base-href /sigesp_pruebas/sigesp_bienes/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sigesp": "npm install sigesp@latest",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/material": "^11.2.13",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@angular/typescript": "^2.0.0-5d0e199",
    "crypto-js": "^4.1.1",
    "moment": "^2.29.1",
    "ngx-toastr": "^11.3.3",
    "pdfmake": "^0.2.7",
    "rxjs": "^6.6.3",
    "sigesp": "^0.9.91-20231211",
    "sweetalert2": "^11.7.12",
    "tslib": "^1.14.1",
    "typescript": "^4.3.5",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular-eslint/builder": "4.3.1",
