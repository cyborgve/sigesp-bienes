    "@angular/compiler-cli": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@angular/localize": "^12.2.17",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^7.6.0",
